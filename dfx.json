{
  "canisters": {
    "patients": {
      "type": "custom",
      "main": "src/canisters/patients/patients.actor.ts",
      "candid": "src/canisters/patients/patients.actor.did",
      "build": "npx azle patients",
      "wasm": ".azle/patients/patients.wasm",
      "gzip": true,
      "declarations": {
        "output": "test/dfx_generated/patients",
        "node_compatibility": true
      }
    },
    "medical-profiles": {
      "type": "custom",
      "main": "src/canisters/medical-profiles/medical-profiles.actor.ts",
      "candid": "src/canisters/medical-profiles/medical-profiles.actor.did",
      "build": "npx azle medical-profiles",
      "wasm": ".azle/medical-profiles/medical-profiles.wasm",
      "gzip": true,
      "declarations": {
        "output": "test/dfx_generated/medical-profiles",
        "node_compatibility": true
      }
    },
    "prescriptions": {
      "type": "custom",
      "main": "src/canisters/prescriptions/prescriptions.actor.ts",
      "candid": "src/canisters/prescriptions/prescriptions.actor.did",
      "build": "npx azle prescriptions",
      "wasm": ".azle/prescriptions/prescriptions.wasm",
      "gzip": true,
      "declarations": {
        "output": "test/dfx_generated/prescriptions",
        "node_compatibility": true
      }
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    }
  }
}
