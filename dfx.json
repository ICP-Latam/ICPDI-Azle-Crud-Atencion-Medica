{
  "canisters": {
    "users": {
      "type": "custom",
      "main": "src/canisters/users/users.canister.ts",
      "candid": "src/canisters/users/users.did",
      "build": "npx azle users",
      "wasm": ".azle/users/users.wasm",
      "gzip": true,
      "declarations": {
        "output": "test/dfx_generated/users",
        "node_compatibility": true
      }
    },
    "patient-profiles": {
      "type": "custom",
      "main": "src/canisters/patient-profiles/patient-profiles.canister.ts",
      "candid": "src/canisters/patient-profiles/patient-profiles.did",
      "build": "npx azle patient-profiles",
      "wasm": ".azle/patient-profiles/patient-profiles.wasm",
      "gzip": true,
      "declarations": {
        "output": "test/dfx_generated/patient-profiles",
        "node_compatibility": true
      }
    },
    "prescriptions": {
      "type": "custom",
      "main": "src/canisters/prescriptions/prescriptions.canister.ts",
      "candid": "src/canisters/prescriptions/prescriptions.did",
      "build": "npx azle prescriptions",
      "wasm": ".azle/prescriptions/prescriptions.wasm",
      "gzip": true,
      "declarations": {
        "output": "test/dfx_generated/prescriptions",
        "node_compatibility": true
      }
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    }
  }
}
